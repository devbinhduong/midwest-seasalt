{"components/halothemes/halo-global":"<div class=\"halo-global-block\">\r\n\t{{> components/halothemes/halo-toolbar-mobile}}\r\n</div>\r\n","components/halothemes/halo-toolbar-mobile":"<div class=\"halo-sticky-toolbar-mobile\">\r\n    <div class=\"bottom-bar\">\r\n        <ul class=\"bottom-bar--list\">\r\n            <li class=\"list-item\">\r\n                <a href=\"/\">\r\n                    <span class=\"icon\"><svg><use xlink:href=\"#icon-toolbar-home\" /></svg></span>\r\n                    <span class=\"text\">Home</span>\r\n                </a>\r\n            </li>\r\n            <li class=\"list-item\">\r\n                <a role=\"link\" aria-disabled=\"true\" data-search-mobile>\r\n                    <span class=\"icon\"><svg><use xlink:href=\"#icon-toolbar-search\" /></svg></span>\r\n                    <span class=\"text\">Search</span>\r\n                </a>\r\n            </li>\r\n            <li class=\"list-item\">\r\n                <a href=\"{{theme_settings.btn_link_collection}}\">\r\n                    <span class=\"icon\"><svg><use xlink:href=\"#icon-toolbar-shop\" /></svg></span>\r\n                    <span class=\"text\">Collection</span>\r\n                </a>\r\n            </li>\r\n            <li class=\"list-item\">\r\n                {{#if customer}}\r\n                    <a href=\"{{urls.account.index}}\" aria-label=\"{{lang 'common.account'}}\">\r\n                        <span class=\"icon\"><svg><use xlink:href=\"#icon-toolbar-account\" /></svg></span>\r\n                        <span class=\"text\">Account</span>\r\n                    </a>\r\n                {{else}}\r\n                    <a href=\"{{urls.auth.login}}\" aria-label=\"{{lang 'common.login'}}\">\r\n                        <span class=\"icon\"><svg><use xlink:href=\"#icon-toolbar-account\" /></svg></span>\r\n                        <span class=\"text\">Account</span>\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n            <li class=\"list-item\">\r\n                <a href=\"{{urls.cart}}\" aria-label=\"{{lang 'cart.nav_aria_label'}}\" data-cart-sidebar>\r\n                    <span class=\"icon\"><svg><use xlink:href=\"#icon-toolbar-cart\" /></svg></span>\r\n                    <span class=\"text\">Cart</span>\r\n                    <div class=\"cart-count-bubble\">\r\n                        <span class=\"text countPill cart-quantity\" aria-hidden=\"true\"></span>\r\n                    </div>\r\n                </a>\r\n            </li>\r\n        </ul>\r\n    </div>\r\n</div>\r\n"}
