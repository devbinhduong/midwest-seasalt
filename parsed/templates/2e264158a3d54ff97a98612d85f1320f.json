{"components/custom/instagram":"\r\n{{#and theme_settings.instagramId theme_settings.instagramKey}}\r\n    <article class=\"instagrame m-b-50 clearfix\" data-section-type=\"storeInfo\">\r\n          <div class=\"insta-image insta-contain\">\r\n            <div class=\"insta-image-inn\">\r\n              <div class=\"insta-img\">\r\n                <img class=\"footer-instalogo-icon\" src=\"/product_images/uploaded_images/instagram-logo.png\"></img>\r\n              </div>\r\n              {{#if theme_settings.instagramtitle}}\r\n              <p class=\"section-sub-title\">{{theme_settings.instagramtitle}}</a></p>\r\n              {{/if}}\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"home-instagram align-center insta-contain\">\r\n              <div class=\"insta-block clearfix\">\r\n                {{#and theme_settings.instagramId theme_settings.instagramKey}}\r\n                  <div id=\"instafeed\" class=\"clearfix\"\r\n                  data-id=\"{{theme_settings.instagramId}}\"\r\n                  data-key=\"{{theme_settings.instagramKey}}\"\r\n                  data-loading=\"{{cdn 'img/loading.svg'}}\">\r\n                  </div>\r\n                {{/and}}\r\n            </div>\r\n          </div>\r\n    </article>\r\n{{/and}}\r\n"}
