{"components/common/mobile-list":"<div class=\"info-section-mb\">\r\n  <ul class=\"navPages-list navPages-list--user\" data-level-list>\r\n    <li class=\"navPages-item navPages-item-user\">\r\n        {{#if customer}}\r\n            <a class=\"navPages-action navPages-action-end\" href=\"{{urls.account.index}}\">\r\n                <span class=\"text\">\r\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.account'}}\">\r\n                        <title>{{lang 'common.account'}}</title>\r\n                        <desc>{{lang 'common.account'}}</desc>\r\n                        <use xlink:href=\"#icon-user\"></use>\r\n                    </svg>\r\n                    {{lang 'account.title'}}\r\n                </span>\r\n            </a>\r\n        {{else}}\r\n            <a class=\"navPages-action navPages-action-end\" href=\"{{urls.auth.login}}\">\r\n                <span class=\"text\">\r\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.account'}}\">\r\n                        <title>{{lang 'common.account'}}</title>\r\n                        <desc>{{lang 'common.account'}}</desc>\r\n                        <use xlink:href=\"#icon-user\"></use>\r\n                    </svg>\r\n                    {{lang 'common.login'}}\r\n                </span>\r\n            </a>\r\n        {{/if}}\r\n    </li>\r\n    <li class=\"navPages-item navPages-item-user\">\r\n        <a class=\"navPages-action navPages-action-end\" href=\"tel:1 513 770 9177\">\r\n            <span class=\"text\">\r\n                <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\r\n                    <use xlink:href=\"#icon-phone\"></use>\r\n                </svg>\r\n               Call Now\r\n            </span>\r\n        </a>\r\n    </li>\r\n    <li class=\"navPages-item navPages-item-user\">\r\n        <a class=\"navPages-action navPages-action-end\" href=\"mailto:contact@midwestseasaltcompany.com\">\r\n            <span class=\"text\">\r\n                <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\r\n                    <title>{{lang 'account.settings.heading'}}</title>\r\n                    <desc>{{lang 'account.wishlists.heading'}}</desc>\r\n                    <use xlink:href=\"#icon-mail\"></use>\r\n                </svg>\r\n                Email Us\r\n            </span>\r\n        </a>\r\n    </li>\r\n      <li class=\"navPages-item navPages-item-user navPages-item-phone\">\r\n          <a class=\"navPages-action navPages-action-end\">\r\n              <span class=\"text\">\r\n                  <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.addresses.phone'}}\">\r\n                      <title>{{lang 'account.addresses.phone'}}</title>\r\n                      <desc>{{lang 'account.addresses.phone'}}</desc>\r\n                      <use xlink:href=\"#icon-heart\"></use>\r\n                  </svg>\r\n                  {{lang 'account.wishlists.heading'}}\r\n              </span>\r\n          </a>\r\n      </li>\r\n      <li class=\"navPages-item navPages-item-user\">\r\n          <a class=\"navPages-action navPages-action-end\" href=\"{{urls.gift_certificate.purchase}}\">\r\n              <span class=\"text\">\r\n                  <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.gift_cert'}}\">\r\n                      <title>{{lang 'common.gift_cert'}}</title>\r\n                      <desc>{{lang 'common.gift_cert'}}</desc>\r\n                      <use xlink:href=\"#icon-gift\"></use>\r\n                  </svg>\r\n                  {{lang 'common.gift_cert'}}\r\n              </span>\r\n          </a>\r\n      </li>\r\n</ul>\r\n</div>\r\n<div class=\"social-section\">\r\n  {{#each social_media}}\r\n  <span class=\"socialLinks-item\">\r\n    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" aria-label=\"Open {{display_name}} in a new tab\">\r\n      <svg class=\"icon\" role=\"img\" aria-label=\"{{display_name}}\">\r\n        <title>{{display_name}}</title>\r\n        <desc>{{display_name}}</desc>\r\n        <use xlink:href=\"#icon-{{display_name}}\"></use>\r\n      </svg>\r\n    </a>\r\n  </span>\r\n  {{/each}}\r\n</div>\r\n"}
