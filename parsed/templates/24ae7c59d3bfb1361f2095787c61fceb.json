{"components/common/requireness-msg":"<small>\n    {{#if required}}*{{/if}}\n</small>\n","components/halothemes/options1/set-rectangle":"<div class=\"form-field\" data-product-attribute=\"set-rectangle\" data-product-option-id=\"{{id}}\" role=\"radiogroup\" aria-labelledby=\"rectangle-group-label\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" id=\"rectangle-group-label\">\n        {{this.display_name}}:\n\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n\n    {{#unless required}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_rectangle__{{../id}}_none\"\n            name=\"attribute[{{../id}}]\"\n            value=\"\"\n            checked=\"{{#if defaultValue '==' ''}}checked{{/if}}\"\n            data-title=\"{{this.label}}\"\n            data-index=\"{{@index}}\"\n        >\n        <label class=\"form-option\" for=\"attribute_rectangle__{{../id}}_none\">\n            <span class=\"form-option-variant\">{{lang 'products.none'}}</span>\n        </label>\n    {{/unless}}\n\n    {{#each this.values}}\n    <div class=\"form-option-wrapper\">    \n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_rectangle__{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}\n            data-title=\"{{this.label}}\"\n            data-index=\"{{@index}}\"\n        >\n        <label class=\"form-option\" for=\"attribute_rectangle__{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\">\n            {{#if this.label.length '>' 2}}\n                <span class=\"form-option-variant form-option-length\">{{this.label}}</span>\n            {{else}}\n                <span class=\"form-option-variant\">{{this.label}}</span>\n            {{/if}}\n        </label>\n    </div>\n    {{/each}}\n</div>\n"}
